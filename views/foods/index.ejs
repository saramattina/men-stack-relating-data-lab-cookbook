<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Pantry</title>
  </head>
  <body>
   <%- include('../partials/_navbar.ejs') %>
    <h1>Welcome To My Pantry</h1>

    <ul>
      <% foods.forEach((food) => { %>
        <a href="/foods/<%= food._id %>">
          <li><%= food.food %></li>
        </a>
      <% }) %>
    </ul>

  </body>
</html>
